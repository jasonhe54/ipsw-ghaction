<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnablePressuredExit</key>
	<true/>
	<key>EnableTransactions</key>
	<true/>
	<key>Label</key>
	<string>com.apple.email.maild</string>
	<key>LaunchEvents</key>
	<dict>
		<key>com.apple.alarm</key>
		<true/>
		<key>com.apple.bg.system.task</key>
		<dict/>
		<key>com.apple.email.ProcessSQLQueryPerformanceData</key>
		<dict>
			<key>AllowBattery</key>
			<true/>
			<key>Interval</key>
			<integer>86400</integer>
			<key>Priority</key>
			<string>Maintainance</string>
			<key>Repeating</key>
			<true/>
		</dict>
		<key>com.apple.xpc.activity</key>
		<dict>
			<key>com.apple.email.maild.sendlaterdelivery.alarmrestart</key>
			<dict>
				<key>AllowBattery</key>
				<true/>
				<key>Delay</key>
				<integer>1</integer>
				<key>GracePeriod</key>
				<integer>1</integer>
				<key>Priority</key>
				<string>Utility</string>
				<key>Repeating</key>
				<false/>
			</dict>
		</dict>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.email.maild</key>
		<true/>
	</dict>
	<key>POSIXSpawnType</key>
	<string>Adaptive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/PrivateFrameworks/EmailDaemon.framework/Versions/A/maild</string>
	</array>
</dict>
</plist>
