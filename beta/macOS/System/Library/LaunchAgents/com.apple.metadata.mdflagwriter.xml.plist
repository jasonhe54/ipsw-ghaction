	<array>
		<string>/System/Library/Frameworks/CoreServices.framework/Versions/A/Frameworks/Metadata.framework/Versions/A/Support/mdflagwriter</string>
	</array>
	<key>ThrottleInterval</key>
	<integer>1</integer>
	<key>_ExponentialThrottling</key>
	<true/>
