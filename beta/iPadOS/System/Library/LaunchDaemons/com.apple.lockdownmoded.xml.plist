				<key>Notification</key>
				<string>com.apple.purplebuddy.setupexited</string>
			</dict>
		</dict>
		<key>com.apple.xpc.activity</key>
		<dict>
			<key>com.apple.lockdownmoded.updateManagedConfiguration</key>
			<dict>
				<key>AllowBattery</key>
				<true/>
				<key>Priority</key>
				<string>Utility</string>
				<key>RandomInitialDelay</key>
				<integer>10</integer>
				<key>Repeating</key>
				<false/>
				<key>RequiresBuddyComplete</key>
				<true/>
			</dict>
			<key>com.apple.lockdownmoded.updateState</key>
			<dict>
				<key>AllowBattery</key>
				<true/>
				<key>Interval</key>
				<integer>3600</integer>
				<key>Priority</key>
				<string>Utility</string>
				<key>Repeating</key>
				<true/>
				<key>RequiresBuddyComplete</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.lockdownmoded</key>
		<true/>
		<key>com.apple.private.alloy.lockdownmode-idswake</key>
		<true/>
		<key>com.apple.usernotifications.delegate.com.apple.LockdownMode</key>
		<true/>
	</dict>
	<key>ProcessType</key>
	<string>Adaptive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/PrivateFrameworks/LockdownMode.framework/lockdownmoded</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>UserName</key>
	<string>mobile</string>
