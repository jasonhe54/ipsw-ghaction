				<true/>
				<key>ResourceIntensive</key>
				<true/>
			</dict>
		</dict>
		<key>com.apple.xpc.activity</key>
		<dict/>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.intelligenceplatform.KnowledgeConstruction</key>
		<true/>
	</dict>
	<key>ProcessType</key>
	<string>Adaptive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/PrivateFrameworks/IntelligencePlatformCore.framework/knowledgeconstructiond</string>
	</array>
	<key>UserName</key>
	<string>mobile</string>
</dict>
</plist>
