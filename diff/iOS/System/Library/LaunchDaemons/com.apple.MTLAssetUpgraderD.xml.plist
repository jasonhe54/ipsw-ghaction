<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnablePressuredExit</key>
	<true/>
	<key>EnableTransactions</key>
	<true/>
	<key>EnvironmentVariables</key>
	<dict>
		<key>MTL_SHADER_CACHE_SIZE</key>
		<string>0</string>
	</dict>
	<key>Label</key>
	<string>com.apple.MTLAssetUpgraderD</string>
	<key>LaunchEvents</key>
	<dict>
		<key>com.apple.distnoted.matching</key>
		<dict>
			<key>Application Installed</key>
			<dict>
				<key>CPUIntensive</key>
				<true/>
				<key>Name</key>
				<string>com.apple.LaunchServices.applicationRegistered</string>
			</dict>
		</dict>
		<key>com.apple.xpc.activity</key>
		<dict>
			<key>com.apple.MTLAssetUpgraderD.os-update</key>
			<dict>
				<key>CPUIntensive</key>
				<true/>
				<key>PostInstall</key>
				<true/>
				<key>Priority</key>
				<string>Maintenance</string>
			</dict>
			<key>com.apple.MTLAssetUpgraderD.prune</key>
			<dict>
				<key>Interval</key>
				<integer>86400</integer>
				<key>Priority</key>
				<string>Maintenance</string>
				<key>Repeating</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.MTLAssetUpgraderD</key>
		<true/>
	</dict>
	<key>POSIXSpawnType</key>
	<string>Background</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/libexec/MTLAssetUpgraderD</string>
	</array>
	<key>UserName</key>
	<string>mobile</string>
</dict>
</plist>
