<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>bundleIdentifier</key>
	<string>com.apple.mlhost.CloudWorker</string>
	<key>name</key>
	<string>com.apple.mlhost.cloud.daily</string>
	<key>taskRequest</key>
	<dict>
		<key>randomInitialDelay</key>
		<real>300</real>
		<key>repeatingTask</key>
		<dict>
			<key>interval</key>
			<real>86400</real>
			<key>minDurationBetweenInstances</key>
			<real>69120</real>
		</dict>
		<key>requiresInexpensiveNetworkConnectivity</key>
		<true/>
		<key>requiresProtectionClass</key>
		<string>ClassC</string>
	</dict>
</dict>
</plist>
