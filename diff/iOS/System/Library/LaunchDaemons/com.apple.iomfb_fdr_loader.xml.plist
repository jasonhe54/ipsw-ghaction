<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnablePressuredExit</key>
	<false/>
	<key>ExitTimeOut</key>
	<integer>5</integer>
	<key>Label</key>
	<string>com.apple.iomfb_fdr_loader</string>
	<key>LaunchEvents</key>
	<dict>
		<key>com.apple.iokit.matching</key>
		<dict>
			<key>com.apple.fdr.load</key>
			<dict>
				<key>IOMatchAll</key>
				<true/>
				<key>IOMatchLaunchStream</key>
				<true/>
				<key>IOPropertyMatch</key>
				<dict>
					<key>IOMFBLaunchFDR</key>
					<true/>
				</dict>
			</dict>
			<key>com.apple.fdrusercal.load</key>
			<dict>
				<key>IOMatchAll</key>
				<true/>
				<key>IOMatchLaunchStream</key>
				<true/>
				<key>IOPropertyMatch</key>
				<dict>
					<key>IOMFBLaunchFDRUserCal</key>
					<true/>
				</dict>
			</dict>
		</dict>
	</dict>
	<key>LimitLoadToSessionType</key>
	<string>System</string>
	<key>ProcessType</key>
	<string>Interactive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/IOMFB_FDR_Loader</string>
		<string>--launchd_boot</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
