		<key>com.apple.metadata.mdflagwriter</key>
		<dict>
			<key>ResetAtClose</key>
			<true/>
		</dict>
	</dict>
	<key>MultipleInstances</key>
	<false/>
	<key>POSIXSpawnType</key>
