				<string>Utility</string>
				<key>Repeating</key>
				<false/>
			</dict>
		</dict>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.linkd.autoShortcut</key>
		<true/>
		<key>com.apple.linkd.cliSupport</key>
		<true/>
		<key>com.apple.linkd.constraints</key>
		<true/>
		<key>com.apple.linkd.extension</key>
		<true/>
		<key>com.apple.linkd.mediator</key>
		<true/>
		<key>com.apple.linkd.observationStatusRegistry</key>
		<true/>
		<key>com.apple.linkd.registry</key>
		<true/>
		<key>com.apple.linkd.suggestions</key>
		<true/>
		<key>com.apple.linkd.transcript</key>
		<true/>
	</dict>
	<key>POSIXSpawnType</key>
	<string>Adaptive</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/libexec/linkd</string>
	</array>
	<key>Umask</key>
	<integer>54</integer>
